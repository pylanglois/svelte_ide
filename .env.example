# Configuration d'authentification OAuth pour l'IDE Svelte

# Providers activés (séparés par des virgules)
VITE_AUTH_PROVIDERS=azure,google

# Configuration Azure AD
VITE_AZURE_CLIENT_ID=your_azure_client_id_here
VITE_AZURE_TENANT_ID=your_azure_tenant_id_here
VITE_AZURE_REDIRECT_URI=http://localhost:5173/auth/callback

# Configuration Google OAuth
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/auth/callback

# Configuration d'environnement
VITE_APP_URL=http://localhost:5173
VITE_APP_KEY=svelte-ide
